{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "build:web": "next build",
    "dev:electron": "concurrently -k -n WEB,ELEC -c blue,magenta \"next dev -p 3000\" \"wait-on http://localhost:3000 && cross-env ELECTRON_START_URL=http://localhost:3000 electron ./electron/main.js\"",
    "build:electron:win": "npx electron-builder --win nsis zip",
    "lint": "next lint",
    "start": "next start"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "1.1.1",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-context-menu": "2.2.4",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-hover-card": "1.1.4",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-menubar": "1.1.4",
    "@radix-ui/react-navigation-menu": "1.2.3",
    "@radix-ui/react-popover": "1.1.4",
    "@radix-ui/react-progress": "1.1.1",
    "@radix-ui/react-radio-group": "1.2.2",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slider": "1.2.2",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/react-tabs": "1.1.2",
    "@radix-ui/react-toast": "1.2.4",
    "@radix-ui/react-toggle": "1.1.1",
    "@radix-ui/react-toggle-group": "1.1.1",
    "@tailwindcss/postcss": "^4.1.9",
    "@vercel/analytics": "latest",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "concurrently": "^9.2.1",
    "date-fns": "4.1.0",
    "dompurify": "latest",
    "electron": "^30.5.1",
    "electron-builder": "^26.0.12",
    "embla-carousel-react": "8.5.1",
    "geist": "latest",
    "html-to-image": "^1.11.13",
    "html2canvas": "1.4.1",
    "input-otp": "1.4.1",
    "jimp": "^1.6.0",
    "lucide-react": "^0.454.0",
    "next": "14.2.33",
    "next-themes": "^0.4.6",
    "postcss": "^8.5",
    "react": "18.3.1",
    "react-day-picker": "9.8.0",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.60.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^2.1.7",
    "recharts": "2.15.4",
    "remark-gfm": "^4.0.1",
    "shadcn": "3.4.0",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^4.1.9",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "wait-on": "^9.0.1",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cross-env": "^10.1.0",
    "tw-animate-css": "1.3.3",
    "typescript": "^5"
  },
  "main": "electron/main.js",
  "build": {
    "appId": "com.mars.onepager",
    "productName": "Mars Onepager",
    "directories": {
      "output": "dist"
    },
    "files": [
      "out/**",
      "electron/main.js",
      "electron/preload.js",
      "package.json"
    ],
    "extraResources": [],
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "artifactName": "${productName}-${version}-${os}-${arch}.${ext}"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "allowToChangeInstallationDirectory": false
    }
  }
}
