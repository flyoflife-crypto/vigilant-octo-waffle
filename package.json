{
  "name": "mars-onepager",
  "version": "1.0.0",
  "private": true,
  "description": "Mars Project One-Pager â€” Next.js + Electron (portable)",
  "type": "commonjs",
  "main": "electron/main.js",
  "scripts": {
    "dev:web": "next dev -p 3000",
    "dev:electron": "cross-env NODE_ENV=development ELECTRON_START_URL=http://localhost:3000 electron .",
    "dev": "concurrently -n WEB,ELEC -c auto \"npm run dev:web\" \"wait-on tcp:3000 && npm run dev:electron\"",
    "build:web": "next build",
    "start:electron": "electron .",
    "pack:win": "electron-builder --win portable --dir --publish=never",
    "dist:win": "electron-builder --win portable --publish=never"
  },
  "build": {
    "appId": "com.mars.onepager",
    "productName": "Mars OnePager",
    "asar": true,
    "files": [
      "electron/**",
      "out/**",
      "package.json"
    ],
    "extraMetadata": {
      "main": "electron/main.js"
    },
    "win": {
      "target": [
        {
          "target": "dir",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "Mars-OnePager-${version}.exe",
      "requestedExecutionLevel": "asInvoker",
      "sign": false,
      "signAndEditExecutable": false
    },
    "directories": {
      "output": "dist"
    }
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-arrow": "^1.1.7",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-focus-scope": "^1.1.7",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-popper": "^1.2.8",
    "@radix-ui/react-portal": "^1.1.9",
    "@radix-ui/react-primitive": "^2.1.3",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-use-layout-effect": "^1.1.1",
    "@radix-ui/rect": "^1.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dompurify": "^3.3.0",
    "embla-carousel-react": "^8.6.0",
    "geist": "^1.5.1",
    "html-to-image": "^1.11.13",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.475.0",
    "next-themes": "^0.3.0",
    "react-day-picker": "^9.11.1",
    "react-hook-form": "^7.65.0",
    "react-markdown": "^9.1.0",
    "react-resizable-panels": "^2.1.9",
    "recharts": "^2.15.4",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^20.16.11",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "electron": "^31.7.7",
    "electron-builder": "^24.13.3",
    "next": "^14.2.33",
    "postcss": "^8.4.47",
    "rcedit": "^4.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^4.1.16",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.6.3",
    "wait-on": "^7.2.0"
  },
  "author": "Saveliy Kuznetsov"
}
